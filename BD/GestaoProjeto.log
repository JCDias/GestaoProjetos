/*C4*/SET SCHEMA PUBLIC
CREATE TABLE DEPARTAMENTOS(\u000a        ID_DEPARTAMENTO INTEGER GENERATED BY DEFAULT AS IDENTITY  PRIMARY KEY,\u000a        NOME VARCHAR(30) NOT NULL,\u000a        CODIGO VARCHAR(3) NOT NULL\u000a)
CREATE TABLE USUARIO(\u000a        ID_USUARIO INTEGER GENERATED BY DEFAULT AS IDENTITY  PRIMARY KEY,\u000a        NOME VARCHAR(30) NOT NULL,        \u000a        EMAIL VARCHAR(50) NOT NULL,\u000a        SENHA VARCHAR(50) NOT NULL,\u000a        CARGO VARCHAR(50) NOT NULL,\u000a        ID_DEPARTAMENTO INTEGER FOREIGN KEY REFERENCES DEPARTAMENTOS(ID_DEPARTAMENTO)\u000a)
CREATE TABLE PROJETO(\u000a        ID_PROJETO INTEGER GENERATED BY DEFAULT AS IDENTITY  PRIMARY KEY,\u000a        NOME VARCHAR(30) NOT NULL, \u000a        DESCRICAO VARCHAR(50) NOT NULL,  \u000a        DATA_INICIO VARCHAR(20) NOT NULL,\u000a        DATA_TERMINIO VARCHAR(20) NOT NULL,\u000a        ID_DEPARTAMENTO INTEGER FOREIGN KEY REFERENCES DEPARTAMENTOS(ID_DEPARTAMENTO)\u000a)
CREATE TABLE ATIVIDADE(\u000a        ID_ATIVIDADE INTEGER GENERATED BY DEFAULT AS IDENTITY  PRIMARY KEY,\u000a        NOME VARCHAR(30) NOT NULL, \u000a        DURACAO FLOAT NOT NULL,  \u000a        ID_PROJETO INTEGER FOREIGN KEY REFERENCES PROJETO(ID_PROJETO),\u000a        ID_USUARIO INTEGER FOREIGN KEY REFERENCES USUARIO(ID_USUARIO)\u000a)
DISCONNECT
